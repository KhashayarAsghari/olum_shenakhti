{
  "name": "cognitive-website",
  "version": "0.0.0",
  "private": true,
  "packageManager": "yarn@4.0.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier -w .",
    "lint": "next lint",
    "prepare": "yarn generate-api",
    "generate-api": "openapi-typescript '../docs/openapi/core.json' -o src/generated/core.d.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.16",
    "@tanstack/react-query": "^5.4.3",
    "react-hook-form": "^7.48.2",
    "next": "14.0.1",
    "openapi-fetch": "^0.8.1",
    "openapi-typescript": "^6.7.0",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "react": "^18",
    "react-dom": "^18",
    "react-toastify": "^9.1.3",
    "stylis-plugin-rtl": "^2.1.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not IE 11"
  ],
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ],
    "printWidth": 120,
    "singleQuote": true,
    "semi": true
  }
}
